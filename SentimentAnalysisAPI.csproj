<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="MLModels\sentiment-data.tsv" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="MLModels\sentiment-data.tsv">
